<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
		 <!-- Sequance generate -->

        <record id="seq_sale_code" model="ir.sequence">
            <field name="name">Sale</field>
            <field name="code">sale_code</field>
            <field name="prefix"></field>
            <field name="padding">4</field>
        </record>

         <record id="seq_invoice_code" model="ir.sequence">
            <field name="name">Invoice</field>
            <field name="code">invoice_code</field>
